{
  "name": "express-typescript-auth",
  "version": "1.0.3",
  "main": "./build/index.js",
  "repository": "https://github.com/rafaelfl/express-typescript-auth.git",
  "author": "Rafael Fernandes Lopes <rafael.fernandes@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf ./build",
    "dev": "nodemon src -e ts,json,yaml --exec 'yarn start-ts'",
    "build": "yarn clean && tsc -p .",
    "start-ts": "npx ts-node --files ./src/index.ts",
    "start": "node ./build/index.js",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "prettier": "prettier --write .",
    "prepare": "husky install",
    "test": "jest --watchAll",
    "test-coverage": "jest --collectCoverage true",
    "test-ci": "jest --ci",
    "db:seed": "npx ts-node --files ./src/seeder/seedDB.ts"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.3.1",
    "morgan": "^1.10.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "redis": "^4.6.7",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.0",
    "winston": "^3.9.0",
    "yaml": "^2.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.3.3",
    "@types/passport": "^1.0.12",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.0.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-standard-with-typescript": "^36.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^27.2.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.3",
    "mockingoose": "^2.16.2",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": ">=3.3.1 <5.1.0"
  },
  "engines": {
    "node": ">=16.15.0"
  }
}
